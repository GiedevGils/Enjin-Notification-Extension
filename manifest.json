{
  "manifest_version": 2,

  "name": "Enjin Notifications",
  "description": "This extension will add Chrome notifications to your Enjin notifications.",
  "version": "1.0","content_scripts": [
    {
      "matches": ["*://*.enjin.com/*"],
      "js": ["index.js"]
    }
  ],

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "vigil_banner.png"
  },

  "permissions": [
   "activeTab",
   "notifications"
   ]
   
}